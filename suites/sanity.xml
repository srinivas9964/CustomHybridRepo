   <!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
   
<suite name="SanityTestSuite" parallel="classes" thread-count="1">
    <listeners>
        <!-- Screenshot on failure -->
        <listener class-name="listeners.TestListener"/>
        <!-- Retry Analyzer -->
        <listener class-name="retryAnalyzer.MyTransformer"/>
    </listeners>
  <test name="SanityTests">
    <classes>
    <class name="tests.LoginTest"/> 
  	<class name="tests.AddEmployeeTest"/> 
      <!-- <class name="tests.PunchTimeTest"/> --> 
    </classes>
  </test>
</suite>


